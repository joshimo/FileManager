<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="fx_shell.fxutility.SearchResultWindowController"
            prefHeight="600.0" prefWidth="600.0">
    <children>
        <TableView fx:id="searchResult" layoutX="10.0" layoutY="10.0" prefHeight="580.0">
            <style>
                -fx-base: lightsteelblue;
                -fx-control-inner-background: rgb(225,225,255);
                -fx-background-color: rgb(170,170,200);
                -fx-effect: dropshadow(gaussian, rgba(175,175,225 0.6), 5, 0.2, 1, 1);
            </style>

            <columns>
                <TableColumn text="Search results:" fx:id="searchResultColumn"/>
            </columns>

        </TableView>
    </children>

</AnchorPane>
